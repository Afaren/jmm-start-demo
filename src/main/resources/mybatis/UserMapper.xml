<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tw.rs.mapper.UserMapper">

    <select id="getUserById" resultType="com.tw.rs.bean.User">
        select * from users where id = #{id}
    </select>

    <select id="getAllUsers" resultType="com.tw.rs.bean.User">
        select * from users
    </select>

</mapper>